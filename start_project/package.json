{
  "name": "cocos-creator-first-game",
  "version": "1.0.0",
  "description": "Cocos Creator First Game Tutorial",
  "main": "index.js",
  "scripts": {
    "lint": "eslint assets/scripts/**/*.ts",
    "lint:fix": "eslint assets/scripts/**/*.ts --fix",
    "format": "prettier --write assets/scripts/**/*.ts",
    "format:check": "prettier --check assets/scripts/**/*.ts",
    "format:all": "prettier --write assets/scripts/**/*.ts && eslint assets/scripts/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "dev": "npm run format:all && npm run type-check"
  },
  "keywords": ["cocos-creator", "game", "typescript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
